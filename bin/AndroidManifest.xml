<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.narrthine.takeawaymalaysia"
    android:versionCode="4"
    android:versionName="1.2" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="15" />

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:debuggable="false"
        android:icon="@drawable/ic_launcher"
        android:label="@string/take_away_malaysia"
        android:theme="@style/CustomTheme" >
        <activity
            android:name="com.narrthine.takeawaymalaysia.MainActivity"
            android:label="@string/take_away_malaysia" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.narrthine.takeawaymalaysia.DisplayAboutActivity"
            android:label="@string/title_activity_display_about"
            android:parentActivityName="com.narrthine.takeawaymalaysia.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.narrthine.takeawaymalaysia.MainActivity" />
        </activity>
    </application>

</manifest>